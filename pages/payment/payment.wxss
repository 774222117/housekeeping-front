  width: 354rpx;
  height: 80rpx;
  background: #fea020;
  border-radius: 87rpx; 
  position: fixed;
  left: 0;
  right: 0;
  bottom: 70rpx;
  margin: 0rpx auto;
  text-align: center;
  line-height: 80rpx;
  font-size: 36rpx;
  font-family: PingFang SC, PingFang SC-Medium;
  font-weight: 500;                                                   
  color: #ffffff;
}